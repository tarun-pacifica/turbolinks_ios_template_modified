platform :ios, '9'

target 'App' do
  use_frameworks!

  pod 'Turbolinks', git: 'https://github.com/turbolinks/turbolinks-ios.git', tag: 'v1.0.3'
  pod 'Fabric'
  pod 'Crashlytics'

  target 'AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AppUITests' do
    inherit! :search_paths
    # Pods for testing
  end
end
